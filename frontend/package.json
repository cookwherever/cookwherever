  "scripts": {
    "start": "node esbuild.config.js --watch",
    "build": "node esbuild.config.js",
    "relay:cleanup": "find src -name __generated__ | xargs rm -rf",
    "relay": "yarn run relay:cleanup && yarn run download:schema && relay-compiler",
    "download:schema": "gq http://localhost:8080/v1/graphql -H \"X-Hasura-Admin-Secret: password\" --introspect > schema.graphql",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "fix": "eslint --ext .js,.jsx,.ts,.tsx  --fix ./src",
    "format": "prettier --write 'src/**/*.{ts,tsx,scss,css,json}'",
    "isready": "yarn format && yarn lint && yarn build",
    "coverage": "yarn test -- --coverage",
    "test:clean": "rimraf ./coverage",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "build:serve": "yarn build && serve -s build",
    "build:profile": "yarn build --profile && serve -s build"
  },
  "resolutions": {
    "@types/react": "^17.0.2",
    "graphql": "15.8.0"
  },
  "eslintConfig": {
    "extends": "react-app"
